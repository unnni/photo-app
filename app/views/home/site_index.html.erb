<h1>Welcome to the Photo App</h1>


<% if !(user_signed_in?) %>
	<p> <%= link_to("Share photos now!", new_user_registration_path) %> </p>
	<p> <%= link_to("Sign in", new_user_session_path) %> </p>
<% else %>
	<p> <%= link_to("Log out", destroy_user_session_path, method: :delete) %> </p>
	<p> <%= link_to("Edit account", edit_user_registration_path) %> </p>
<% end %>